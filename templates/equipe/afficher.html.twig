
{% block title %}Equipe index{% endblock %}

{% block body %}
    <h1>Equipe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for equipe in equipes %}
            <tr>
              <td>{{ equipe.getId() }}</td>
              <td>{{ equipe.getName() }}</td>

                <td>
                  <a href="{{ path('app_equipe_show', {'id': equipe.getId()}) }}">show</a>
<a href="{{ path('app_equipe_edit', {'id': equipe.getId()}) }}">edit</a>

                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_equipe_new') }}">Create new</a>
{% endblock %}
